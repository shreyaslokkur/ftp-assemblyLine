<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.lks.orm.entities.Branch" table="branch" catalog="test">

        <id name="branchName" type="string">
            <column name="branchName" length="45" not-null="true" />
            <generator class="assigned" />
        </id>
        <property name="branchAddress" type="string">
            <column name="password" length="60" not-null="true" />
        </property>
        <property name="enabled" type="boolean">
            <column name="enabled" not-null="true" />
        </property>

    </class>
</hibernate-mapping>